<h1>Editing deposit</h1>

<% form_for(@deposit) do |f| %>
  <%= f.error_messages %>


  <p>
    <%= f.label :entityid %><br />
    <%= f.text_field :entityid %>
  </p>

  <p>
    <%= f.label :longitude %><br />
    <%= text_field_tag :longitude, @deposit.geom.sdo_point.x %>
  </p>
  <p>
    <%= f.label :latitude %><br />
    <%= text_field_tag :latitude, @deposit.geom.sdo_point.y %>
  </p>


  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @deposit %> |
<%= link_to 'Back', deposits_path %>